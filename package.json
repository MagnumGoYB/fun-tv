{
  "name": "fun-tv",
  "version": "0.0.0",
  "description": "一个有趣的 Top20 电影榜单",
  "keywords": [
    "top20",
    "movie",
    "list"
  ],
  "homepage": "https://github.com/MagnumGoYB/fun-tv#readme",
  "bugs": {
    "url": "https://github.com/MagnumGoYB/fun-tv/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MagnumGoYB/fun-tv.git"
  },
  "license": "MIT",
  "author": "MagnumGoYB (sosbs@outlook.com)",
  "scripts": {
    "start": "parcel src/index.html --open",
    "clean": "rm -rf dist/*",
    "prepare": "husky install",
    "sort": "sort-package-json",
    "prettier": "prettier -c --write '**/*'",
    "build:parcel": "parcel build src/index.html --no-content-hash --no-minify --no-source-maps --public-url ./",
    "build": "npm run clean && npm run build:parcel"
  },
  "dependencies": {
    "gsap": "^3.6.1",
    "imagesloaded": "^4.1.4"
  },
  "devDependencies": {
    "commitlint": "^11.0.0",
    "commitlint-config-gitmoji": "^2.2.5",
    "eslint": "^7.28.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.3.1",
    "sort-package-json": "^1.50.0"
  }
}
